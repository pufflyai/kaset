{
  "id": "file-explorer",
  "name": "File Explorer",
  "version": "1.1.0",
  "api": "v1",
  "entry": "index.js",
  "description": "Browse and edit files stored in the playground workspace.",
  "dependencies": {
    "@chakra-ui/react": "https://esm.sh/@chakra-ui/react@3.24.2?deps=react@19.1.1,react-dom@19.1.1,@emotion/react@11.14.0,@emotion/styled@11.14.0,framer-motion@12.1.0",
    "@emotion/react": "https://esm.sh/@emotion/react@11.14.0?deps=react@19.1.1",
    "lucide-react": "https://esm.sh/lucide-react@0.542.0?deps=react@19.1.1",
    "react": "https://esm.sh/react@19.1.1/es2022/react.mjs",
    "react-dom": "https://esm.sh/react-dom@19.1.1/es2022/react-dom.mjs",
    "react-dom/client": "https://esm.sh/react-dom@19.1.1/es2022/client.mjs",
    "react/jsx-runtime": "https://esm.sh/react@19.1.1/es2022/jsx-runtime.mjs"
  },
  "commands": [
    {
      "id": "fileExplorer.openFolder",
      "title": "Open Folder",
      "category": "File Explorer",
      "description": "Show a folder inside the File Explorer window.",
      "parameters": {
        "type": "object",
        "properties": {
          "path": {
            "type": "string",
            "description": "Folder path relative to the playground root to display."
          }
        },
        "required": ["path"],
        "additionalProperties": false
      }
    }
  ],
  "surfaces": {
    "desktop": {
      "title": "File Explorer",
      "description": "Inspect files beneath the playground root and edit them in place.",
      "icon": "folder-closed",
      "singleton": false,
      "defaultSize": {
        "width": 840,
        "height": 620
      },
      "window": {
        "entry": "src/window.tsx"
      }
    }
  }
}
